datatype float
	linkage 
		cpp_part f
	default 0.0
		
	module
		function Max (|float|)
			cpp_wrapper JB_f__max
		function Min (|float|)
			return -.max
		function Nan (|float|)
			cpp_wrapper JB_f__nan
	
	render
		return .Render(6, fs_in)

	Render RenderDuration (|bool| ms = false)
		// perhaps just cast this to an ACTUAL duration
		// AND THEN DO THE CAST!?
		if self < 0
			self = .length
			fs <~ '-'
		|| s = self mod 60.0
		|| m = (.floor - s) / 60.0
		if m > 0
			fs <~ (m+0.5)|int|
			fs <~ 'm'
			fs <~ ' '
		fs.AppendFloat(s, 1+2*ms, false)
		fs <~ 's'
	
	render (|int| dp)
		cpp_part RenderDP
		fs.AppendFloat(self, dp)


	render Percent // operator percent, operator pc 
		if !self
			fs <~ "0"
		  elseif self < 0.001
			fs <~ "<0.1"
		  else
			fs.appendfloat(self*100.0, 1)
		fs <~ "%"
	
