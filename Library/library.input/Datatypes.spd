//		trin (19, CLM)
		cpp_wrapper JB_f_Clamp
		asm fclm(dest, p1, p2, p3, 1)

	function Mix (|float| A, |float| B, |float|)
		cpp_Part FloatMix
		trin (20, MIX)
		description "0.0 means fully A,  1.0 means fully B,  0.5 means half-way between A and B." 
		return A + (self * (B - A))

	function SmoothStep (|float| low=0, |float| high=1, |float|)
		trin (21, SMS)
		|| t = ((self - low) / (high - low)).clamp
		return t * t * (3.0 - 2.0 * t)
	
	function IsNan (|int|)
		trin (22, XNN)
		cpp_wrapper (__isnanf)
	
	function IsInf (|int|)
		trin (23, XNF)
		cpp_wrapper (__isinff)

	operator ShiftLeft (|int| x, |float|)
		while --x >= 0	// there is a faster way, but this is good enough.
			self *= 2.0	// mostly people wanna do x << 1 or x << 2
		return self
			
	operator ShiftRight (|int| x, |float|)
		while --x >= 0
			self *= 0.5
		return self
	
	function vec2.operatorby (|float| step,  |FloatRange|)
		rz.From = .x
		rz.To = .y
		rz.step = step
	
	operator to (|float| To, |floatrange|)
		rz.from = self
		rz.to = to
		rz.step = 1.0

	iterator
		|| i = 0.0
		while (i < self)
			yield i (i)
			++i




