		trin (10, LG2)
		cpp_wrapper (log2f)

	function Unsin (|float|)
		trin (12, NIS)
		return math.unsinebad(self) * math.iTau

	function Uncos (|float|)
		trin (13, SOC)
		return math.uncosbad(self) * math.iTau

	function UnTan (|float| y, |float|)
		trin (14, NAT)
		rz = math.unTan2bad(self, y)			// our result is +-pi, but we want 0-1...
		rz *= -math.iTau
		if rz < 0.0
			rz ++

	function Sine (|float|)
		trin (15, SIN)
		return math.sinpihalf(self*2.0)

	function Cos (|float|)
		trin (16, COS)
		return math.cospihalf(self*2.0)

	function Tan (|float|)
		trin (17, TAN)
