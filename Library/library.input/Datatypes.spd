	function Trim (|int| B, |uint64|)
		B = 64-B
		return (self << B) >> B

	function XorDownTo8Bits (|byte|)
		|uint| x = (self >> 32) xor self
		x = (x >> 16) xor x
		x = (x >> 8) xor x
		return x

	function IsPow2 (|bool|)
		cpp_wrapper JB_u64_IsPow2

	function HighestBit (|uint64|)
		|| l = .log2
		if l >= 0
