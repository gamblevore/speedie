struct RetroFloat
// for doing pixel-based animations that move smoothly for retro-games
	|float|		Amount
	|float|		Remain
	
	syntax Append (|float| n)
		.Amount += n
	
	syntax Access (|int|)
		|| A = .Amount // 1.1... every 10th frame returns 2
		|| rem = A.fract + .remain
		if rem >= 1.0
			--rem
			++A
		.remain = rem
		return A|int|

	
function float.Steps (|FloatRange|)
	rz.from = -1
	rz.to = 1
	rz.Step = 2.0 / self


struct IntDownRange   /// :3
	|int| From
	|int| To
	
	iterator
		|| _c = .from
		while (_c >= .To)
			yield _c (_c)
			--_c


struct FloatRange   /// :3
	|Float| From
	|Float| To
	|float| Step
	
	autogen float.LoopOK (|float| Final, |float| Step, |bool|)
		opt inline
		if Step >= 0.0
			return self <= final
		return self >= final
		
	iterator
		|| _c = .from
		while (_c.LoopOK(.To, .step))
			yield _c (_c)
			_c += .Step


