module abc
	task_run HelloSayer
		"hello" // will print "hello" when run

	task_run StringSayer (|string| str)
		printline .str
	
	task RandomSayer (|string| name)
		|uint16| RandomID = 0
		constructor
			.randomid = random.shared.int
		run
			"$.name $.randomid"
