datatype GUIKeyRecordInfo (byte)
	constants
		|GUIKeyRecordInfo|  
		Empty		= 0
		ToRemove	= 1
		ToAdd		= 2
		NewlyAdded	= 3
		Stable		= 4


struct GUIKeyRecord
	|date| Date
	|keys| Key
	|GUIKeyRecordInfo| State

	function Clear
		.date = nil
		.key = nil
		.state = nil
	
	syntax is (|GUIKeyRecordInfo| V, |bool|)
		return .state == v


