

datatype KeyMod (int)
	linkage
		wrapper
		cpp_class SDL_Keymod
		cpp_part KM

	syntax cast (|bool|)
		return (self|uint|) & (sdlapp.keymod|uint|)

	constants
		|Keymod|
		LSHIFT	= 0x0001
		RSHIFT	= 0x0002
		LCTRL	= 0x0040
		RCTRL	= 0x0080
		LALT	= 0x0100
		RALT	= 0x0200
		LGUI	= 0x0400
		RGUI	= 0x0800
		NUM		= 0x1000
		CAPS	= 0x2000
		MODE	= 0x4000

		CTRL	= LCTRL	 + RCTRL
		CMD		= LGUI	 + RGUI
		Shift	= Lshift + Rshift
		Option	= Lalt	 + Ralt


dataType SDL_Scancode (int)
	linkage
		wrapper
		cpp_part Scan
	
	function Key (|keys|)
		cpp_wrapper SDL_GetKeyFromScancode

	function Pressed (|bool|)
		|| n = 0
		|| state = keys.state(n)|&byte|
		if n > self
			return state[self]



struct SDL_KeyboardEvent
	linkage: wrapper
	|SDLEventType|				Type
	|uint|						Timestamp
	|SDL_Keysym|				keysym
	
	function scan (|SDL_Scancode|)
		return .keysym.scancode

	function key (|keys|)
		return .keysym.sym

	syntax is (|keys| x, |bool|)
		return .key == x



struct SDL_Keysym
	linkage
		wrapper
		cpp_part KS
    |SDL_Scancode|	scancode		/**< SDL physical key code - see ::SDL_Scancode for details */
    |keys|			sym				/**< SDL virtual key code - see ::SDL_Keycode for details */
    |Uint16|		mod				/**< current key modifiers */
    |Uint|			unused

    function Command (|bool|)
		return .mod & (1024(|)64(|)128(|)2048)
	



dataType Keys (int) {
	linkage
		wrapper
		cpp_class SDL_Keycode
		cpp_part Key


	module
		|keys[8]|				DownKeys
		|date[8]|				DownDate
		|int|					Count

		function State (|&int| n, |&byte|)
			cpp_Wrapper SDL_GetKeyboardState
		
		function keys.eat
			|| i = keys.has(self)
				keys.downdate[i] = 0

		function keys.HeldKey (|date| durr=1, |bool|)
			|| i = keys.has(self)
				return keys.downdate[i].ago >= durr

		function keys.iscancelled (|bool|)
			|| i = keys.has(self)
				if !keys.downdate[i]
					return true
			
		function AddKey (|keys| K)
			if (.count < 8) and !.has(K)
				|| i = .count++
				.DownDate[i] = date()
				.DownKeys[i] = K
				
		function RemoveKey (|keys| K)
			for i in .count
				if .downkeys[i] == K
					|| m = --.count
					.downkeys[i] = .downkeys[m]
					.downkeys[m] = 0
			
		function has (|keys| s, |ind|)
			for i in 8
				if .downkeys[i] == s
					return i

		function Cmd (|bool|)
			if KeyMod.ctrl
				return true
			if !platform.windows
				return keymod.cmd

		function RealCmd (|bool|)
			return keymod.cmd

		function PlatCmd (|bool|yes=true,|bool|)
			if platform.osx==yes
				return keymod.cmd
			  else
				return KeyMod.ctrl

		function Control (|bool|)
			return KeyMod.ctrl

		function Shift (|bool|) 
			return KeyMod.shift			

		function Cancel (|bool|) 
			return (cancelkey or escape or AC_BACK)

		function Option (|bool|) 
			return KeyMod.option		


	function Scan (|SDL_Scancode|)
		cpp_wrapper SDL_GetScancodeFromKey
			
	function IsAccept (|bool|)
		return (self is RETURN or RETURN2 or Enter or AC_FORWARD)
	
	operator iscmd (|keys|s, |bool|)
		return keys.cmd and self == s

	function IsCMM  (|bool|shift=false,|bool|)
		if shift == (keys.shift or keys.option)
			return self is section or MacBackQuote or backquote or APPLICATION or slash

	function IsModifier (|bool|)
		return (self is lgui or rgui or lctrl or rctrl or lalt or ralt or lshift or rshift)

	function IsCancel (|bool|) 
		return (self is cancelkey or escape or AC_BACK)

	function isdelete (|bool|)
		return self is forwarddelete or delete or kp_delete

	function IsShift (|bool|)
		return self is lshift or rshift
		
	function IsScan (|bool|)
		return ((1<<30) & self)!=0

	function IsOption (|bool|)
		return self is lalt or ralt
		
	syntax access (|bool|)
		return keys.has(self)
	
	syntax cast (|bool|)
		return keys.has(self)


	syntax is (|keys| x, |bool|)
		return self == x

	syntax isnt (|keys| x, |bool|)
		return self != x

