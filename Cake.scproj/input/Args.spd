
	
function cake.Collect (|File?|)
	opt norefcounts
	|| r = app._rawargs
	|| r1 = r[0]
	if r1.ArgSwitch("d") // remove "d" eventually!
		debugger.WantDebug = true
		r1 = r[1]

	if !r1
		quit "Usage: ${app.filename} /path/to/spd_proj.cake

Passed cake files are run.
Passed speedie projects are compiled then run."
		
	check (r1[] != '-') ("Unexpected switch: " + r1)
	rz = r1.FileThatExists("load executable")
	if debugger.WantDebug
		r.Remove(0)
	r.Remove(0)
	

	
