
	
function cake.Collect (|File?|)
	opt norefcounts
	|| r = app._rawargs
	|| r1 = r[0]
	if r1.ArgSwitch("d") // remove "d" eventually!
		debugger.WantDebug = true
		r1 = r[1]

	expect (r1)	"Usage: ${app.filename} /path/to/spd_proj.cake
May also pass .spd or .scproj files. They will be compiled, then run."
		
	check (r1[] != '-') ("Unexpected switch: " + r1)
	rz = r1.FileThatExists("load executable")
	if debugger.WantDebug
		r.Remove(0)
	r.Remove(0)
	

	
