extend Message
	function PrevIndentCheck (|message| pr, |int|)
		Opt NoRefCounts
		|| chin = .Indent&~1
		if chin >= 252
			chin = 0
		|| prin = pr.indent&~1
		if prin >= 252
			pr.indent = .indent
			prin = chin
		
		|| IndExtra = chin - prin				#require
		if (indExtra isa 2)
			if indextra == 4
				return IndExtra
			if IndExtra < 0
				if (indextra isa 4) or (.indent|int| isa 4) or (.InInvisArg)
					return indextra
			if (IndExtra > 0) and (pr == @tmp)
				return IndExtra

		parser.ErrorAlwaysAdd( "Indents mismatch (${prin|int|} vs ${chin|int|} spaces)", .position )


