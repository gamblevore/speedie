#!/usr/local/bin/spd


main (|File| File, |int| N)
	|| fs = file.OutputStream$
	for i in n
		fs.AppendRandomWord(5, 12)
		fs <~ "\n"
	fs.flush
	"Created ${File.size.strsize} at $File"


function faststring.AppendRandomWord (|int| Min, |int| Max)
	|| n = random.IntRange(Min, Max)
	Random.Text(self, n, 'a', 'z')
