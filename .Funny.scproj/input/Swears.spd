

class SwearWord
	|string|	Name
	|bool| 		Adjective

	constructor (|string| s, |error| anything)
		if s[] == '+'
			s = s[1...]
			.Adjective = true
		.name = s


extend_module swearword
	|[SwearWord]|					First
	|[SwearWord]|					Second
	|bool| 							AllowSame
	|bool| 							Print

	helper LoadTo (|[swearword]| list, |string| s)
		for w in s.unbleep.Split
			if w
				list <~ SwearWord(w, nil|error|)
		string.sort(list)
	
	helper Accept (|SwearWord| a, |SwearWord| b, |bool|)
		require a != b
		if .AllowSame
			require a.name[] == b.name[]
		return true
		
	function New (|string|)
		return new("", 1)
	function New (|int| n = 1,  |string|)
		return new("", n)
	function New (|int| a, |int| b,  |string|)
		return new("", a+b)
	
	function New (|string| upon, |int| n = 1,  |string|)
		if !n
			return upon
		if !.first
			.loadto(.First, firstwords)
		if !.second
			.loadto(.second, secondwords)
		|| fs = FastString()
		if upon
			fs <~ upon
			fs <~ ' '
		|| done = false
		for i in n
			for 100 
				|| w1 = .first.now	#loop
				|| w2 = .Second.now	#loop
				if .accept(w1, w2)
					if done
						fs <~ ", "
					done = true
					if 0.5[]
						for 50
							|| extra = .First[]!
							if extra.Adjective and .Accept(w1, extra) and .Accept(w2, extra)
								fs <~ extra.name
								fs <~ " "
								exit
							
					fs <~ w1.name
					fs <~ " "
					fs <~ w2.name
					exit
		return fs
		
			
