

module CakeArgs
	|| WantDebug = false
	
	function Collect (|File?|)
		|| r = app._rawargs
		|| r1 = r[0]
			r.Remove(0)
			if (r1.ArgName == "d") and r1.ArgValue.Yes
				.WantDebug = true
				r1 = r[0]
				r.Remove(0)
		if !r1
			"Usage: ${app.filename} /path/to/spd_proj.cake
	May also pass .spd or .scproj files. They will be compiled, then run.\n"
		  else
			check (!r1.ArgName) "Unexpected switch: " + r1
			rz = r1.FileThatExists("load executable")
		

		
