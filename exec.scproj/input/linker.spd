

module SpdFnTable 
	constants	
		FnLim = 10K // its about corrupted files, not "we can't handle more"!
	
	|[SpdFunc]|	Funcs

	syntax access (|int| i, |SpdFunc|)
		return .Funcs[i]
	
	function RunMain (|int|)
		opt norefcounts
		|| main = .funcs[1]					#expect ("Missing main")
		rz  = .funcs[0]!.Run|&int64|[]
		rz ?= main.Run|&int64|[]
