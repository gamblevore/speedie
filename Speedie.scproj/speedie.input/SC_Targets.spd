module Targets
	|| Items = dictionary()
	function Set (|message| tmp)
		cpp_part SetMsg
		|| thg = tmp[@thg]
			.set(thg.name)
	function Set (|string| name)
		.items[name] = "true"
	syntax Access (|string| name, |bool|)
		return targets.items.valuelower(name)
